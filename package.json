{
  "name": "resub-persist",
  "version": "1.2.4",
  "description": "Persist and Rehydrate your Resub-Stores!",
  "main": "dist",
  "typings": "dist",
  "repository": "https://github.com/Hizoul/resub-persist.git",
  "author": "MMB2 <git@mmb2.click>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "coveralls": "^3.0.0",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && ./node_modules/.bin/tsc",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --ci --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "react": "preact-compat",
      "react-dom": "preact-compat"
    },
    "mapCoverage": true
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "resub": "^1.0.7"
  }
}
