{
  "name": "resub-persist",
  "version": "1.1.8",
  "description": "Persist and Rehydrate your Resub-Stores!",
  "main": "dist",
  "typings": "dist",
  "repository": "https://github.com/Hizoul/resub-persist.git",
  "author": "MMB2 <git@mmb2.click>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "ts-jest": "^21.2.1",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && tsc",
    "build": "tsc",
    "test": "./node_modules/.bin/jest --ci --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "react": "preact-compat",
      "react-dom": "preact-compat"
    },
    "mapCoverage": true
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "resub": "^0.0.21"
  }
}
