{
  "name": "resub-persist",
  "version": "1.2.8",
  "description": "Persist and Rehydrate your Resub-Stores!",
  "main": "dist",
  "typings": "dist",
  "repository": "https://github.com/Hizoul/resub-persist.git",
  "author": "MMB2 <git@mmb2.click>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "preact": "^10.4.1",
    "preact-compat": "^3.18.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.1.6",
    "resub": "^1.0.13"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && ./node_modules/.bin/tsc",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --ci --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "react": "preact-compat",
      "react-dom": "preact-compat"
    }
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "peerDependencies": {
    "resub": "1.x.x"
  }
}
